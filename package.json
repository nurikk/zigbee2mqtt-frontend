{
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": [
        "*.css"
    ],
    "name": "zigbee2mqtt-frontend",
    "version": "0.3.92",
    "license": "GPL-3.0",
    "scripts": {
        "build": "npm run clean && webpack --config ./webpack.config.js --mode production",
        "start": "concurrently 'npm run start:ws' 'npm run start:dev'",
        "start:dev": "webpack serve --config ./webpack.config.js --mode development",
        "start:ws": "node ws.js",
        "clean": "rimraf ./dist",
        "postversion": "git push && git push --tags",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
        "postinstall": "husky install",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable"
    },
    "eslintIgnore": [
        "dist/*"
    ],
    "devDependencies": {
        "@commitlint/cli": "^12.0.1",
        "@commitlint/config-conventional": "^12.0.1",
        "@fortawesome/fontawesome-free": "^5.15.1",
        "@rjsf/bootstrap-4": "https://github.com/nurikk/fileshare/raw/main/rjsf-bootstrap-4-2.5.0.tgz",
        "@rjsf/core": "https://github.com/nurikk/fileshare/raw/main/rjsf-core-2.5.1.tgz",
        "@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
        "@types/classnames": "^2.2.10",
        "@types/color-convert": "^2.0.0",
        "@types/d3-drag": "^2.0.0",
        "@types/d3-force": "^2.1.1",
        "@types/d3-selection": "^2.0.0",
        "@types/d3-zoom": "^2.0.0",
        "@types/lodash": "^4.14.167",
        "@types/react": "^17.0.1",
        "@types/react-bootstrap": "^0.32.25",
        "@types/react-dom": "^17.0.0",
        "@types/react-router-dom": "^5.1.7",
        "@types/reconnectingwebsocket": "^1.0.6",
        "@types/webpack-env": "^1.16.0",
        "@types/ws": "^7.4.0",
        "@typescript-eslint/eslint-plugin": "^4.17.0",
        "@typescript-eslint/parser": "^4.17.0",
        "ace-builds": "^1.4.12",
        "bootstrap": "^5.0.0-alpha3",
        "classnames": "^2.2.6",
        "color-convert": "^2.0.1",
        "concurrently": "^6.0.0",
        "conventional-changelog-cli": "^2.1.1",
        "copy-webpack-plugin": "^8.0.0",
        "css-loader": "^5.1.2",
        "d3-drag": "^2.0.0",
        "d3-force": "^2.1.1",
        "d3-force-reuse": "^1.0.1",
        "d3-selection": "^2.0.0",
        "d3-zoom": "^2.0.0",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "fork-ts-checker-webpack-plugin": "^6.1.0",
        "html-webpack-plugin": "^5.2.0",
        "husky": "^5.1.1",
        "line-reader": "^0.4.0",
        "lodash": "^4.17.21",
        "mini-css-extract-plugin": "^1.3.9",
        "notyf": "^3.9.0",
        "pinst": "^2.1.6",
        "prettier": "^2.2.1",
        "react": "^17.0.1",
        "react-ace": "^9.3.0",
        "react-bootstrap": "^1.4.3",
        "react-css-theme-switcher": "^0.2.2",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0",
        "reconnecting-websocket": "^4.4.0",
        "rimraf": "^3.0.2",
        "sass": "^1.32.8",
        "sass-loader": "^11.0.1",
        "timeago.js": "^4.0.2",
        "ts-loader": "^8.0.17",
        "typescript": "^4.2.3",
        "unistore": "^3.5.2",
        "webpack": "^5.25.0",
        "webpack-bundle-analyzer": "^4.3.0",
        "webpack-cli": "4.5.0",
        "webpack-dev-server": "3.11.2",
        "zigbee-herdsman": "^0.13.78",
        "zip-webpack-plugin": "^4.0.1"
    },
    "main": "index.js",
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nurikk/z2m-frontend.git"
    },
    "bugs": {
        "url": "https://github.com/nurikk/z2m-frontend/issues"
    }
}
