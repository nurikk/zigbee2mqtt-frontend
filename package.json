        "react": "^17.0.1",
        "react-ace": "^9.5.0",
        "react-app-polyfill": "^3.0.0",
        "react-css-theme-switcher": "^0.3.0",
        "react-dom": "^17.0.1",
        "react-i18next": "^11.14.3",
        "react-image": "^4.0.3",
        "react-router-dom": "^5.3.0",
        "react-table": "^7.7.0",
        "react-transition-group": "^4.4.2",
        "reconnecting-websocket": "^4.4.0",
        "rimraf": "^3.0.2",
        "sass": "^1.49.7",
        "sass-loader": "^12.4.0",
        "timeago.js": "^4.0.2",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.5",
        "unistore": "^3.5.2",
        "webpack": "^5.68.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "4.9.1",
        "webpack-dev-server": "4.7.4",
