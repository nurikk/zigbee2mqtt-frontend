{
    "private": false,
    "publishConfig": {
        "access": "public",
        "cache": ""
    },
    "sideEffects": [
        "*.css"
    ],
    "name": "zigbee2mqtt-frontend",
    "version": "0.6.9",
    "license": "GPL-3.0",
    "scripts": {
        "build": "npm run clean && webpack --config ./webpack.config.js --mode production",
        "start": "concurrently 'npm run start:ws' 'npm run start:dev'",
        "start:dev": "webpack serve --config ./webpack.config.js --mode development",
        "start:ws": "node ws.js",
        "clean": "rimraf ./dist",
        "postversion": "git push && git push --tags",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
        "postinstall": "husky install",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable"
    },
    "eslintIgnore": [
        "dist/*"
    ],
    "devDependencies": {
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@fortawesome/fontawesome-free": "^5.15.4",
        "@rjsf/bootstrap-4": "^3.1.0",
        "@rjsf/core": "^3.1.0",
        "@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
        "@types/classnames": "^2.3.1",
        "@types/color-convert": "^2.0.0",
        "@types/d3-drag": "^3.0.1",
        "@types/d3-force": "^3.0.3",
        "@types/d3-selection": "^3.0.1",
        "@types/d3-zoom": "^3.0.1",
        "@types/file-saver": "^2.0.3",
        "@types/lodash": "^4.14.175",
        "@types/react": "^17.0.25",
        "@types/react-bootstrap": "^0.32.26",
        "@types/react-dom": "^17.0.9",
        "@types/react-router-dom": "^5.3.0",
        "@types/react-transition-group": "^4.4.3",
        "@types/reconnectingwebsocket": "^1.0.7",
        "@types/webpack-env": "^1.16.2",
        "@types/ws": "^8.2.0",
        "@typescript-eslint/eslint-plugin": "^4.32.0",
        "@typescript-eslint/parser": "^4.32.0",
        "ace-builds": "^1.4.12",
        "bootstrap": "^5.1.1",
        "classnames": "^2.2.6",
        "color-convert": "^2.0.1",
        "concurrently": "^6.2.1",
        "conventional-changelog-cli": "^2.1.1",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^6.3.0",
        "d3-drag": "^3.0.0",
        "d3-force": "^3.0.0",
        "d3-force-reuse": "^1.0.1",
        "d3-selection": "^3.0.0",
        "d3-zoom": "^3.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.0",
        "file-saver": "^2.0.5",
        "fork-ts-checker-webpack-plugin": "^6.3.3",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^7.0.2",
        "i18next": "^21.2.0",
        "i18next-browser-languagedetector": "^6.1.2",
        "i18next-http-backend": "^1.3.1",
        "jszip": "^3.7.1",
        "line-reader": "^0.4.0",
        "lodash": "^4.17.21",
        "mini-css-extract-plugin": "^2.3.0",
        "notyf": "^3.10.0",
        "pinst": "^2.1.6",
        "prettier": "^2.4.1",
        "react": "^17.0.1",
        "react-ace": "^9.4.4",
        "react-bootstrap": "^1.6.1",
        "react-css-theme-switcher": "^0.3.0",
        "react-dom": "^17.0.1",
        "react-i18next": "^11.12.0",
        "react-router-dom": "^5.3.0",
        "react-transition-group": "^4.4.2",
        "reconnecting-websocket": "^4.4.0",
        "rimraf": "^3.0.2",
        "sass": "^1.42.1",
        "sass-loader": "^12.1.0",
        "timeago.js": "^4.0.2",
        "ts-loader": "^9.2.6",
        "typescript": "^4.4.3",
        "unistore": "^3.5.2",
        "webpack": "^5.55.1",
        "webpack-bundle-analyzer": "^4.4.2",
        "webpack-cli": "4.8.0",
        "webpack-dev-server": "4.3.0",
        "zigbee-herdsman": "^0.13.149",
        "zip-webpack-plugin": "^4.0.1"
    },
    "main": "index.js",
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nurikk/z2m-frontend.git"
    },
    "bugs": {
        "url": "https://github.com/nurikk/z2m-frontend/issues"
    }
}
