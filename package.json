    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nurikk/zigbee2mqtt-frontend.git"
    },
    "bugs": {
        "url": "https://github.com/nurikk/zigbee2mqtt-frontend/issues"
    }
}
